function TWideStrings.GetTextStr: WideString;

var
  I, L,
  Size,
  Cnt: Integer;
  P: PWideChar;
  S: WideString;

begin
  Cnt := GetCount;
  Size := 0;
  for I := 0 to Cnt - 1 do Inc(Size, Length(Get(I)) + 2);
  SetLength(Result, Size);
  P := Pointer(Result);
  for I := 0 to Cnt - 1 do
  begin
    S := Get(I);
    L := Length(S);
    if L <> 0 then
    begin
      System.Move(Pointer(S)^, P^, 2 * L);
      Inc(P, L);
    end;
    P^ := CarriageReturn;
    Inc(P);
    P^ := LineFeed;
    Inc(P);
  end;
end;

